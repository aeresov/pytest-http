{
    "login": {
        "name": "Login and acquire session key",
        "vars": {
            "service": "user",
            "method": "login"
        },
        "request": {
            "$ref": "../requests.json#/service_call",
            "method": "GET",
            "params": {
                "username": "{{ username }}",
                "auth": "{{ password }}",
                "slogin": true
            }
        },
        "response": [
            {
                "verify": {
                    "status": 200
                }
            },
            {
                "save": {
                    "vars": {
                        "session_key": "User.SessionKey"
                    }
                }
            }
        ]
    },
    "logout": {
        "name": "Logout with session key",
        "always_run": true,
        "vars": {
            "service": "user",
            "method": "logout"
        },
        "request": {
            "$ref": "../requests.json#/service_call_session",
            "method": "GET"
        },
        "$ref": "../../common/responses/status.json#/ok"
    }
}