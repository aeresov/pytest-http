{
    "service_call": {
        "url": "{{ api_base }}/services/{{ service }}/{{ method }}/",
        "params": {
            "format": "json"
        },
        "auth": {
            "function": "utils.auth:altec_auth",
            "kwargs": {
                "method": "{{ method }}"
            }
        },
        "ssl": {
            "verify": false
        }
    },
    "service_call_session": {
        "$ref": "#/service_call",
        "headers": {
            "X-Session-Key": "{{ session_key }}"
        }
    }
}